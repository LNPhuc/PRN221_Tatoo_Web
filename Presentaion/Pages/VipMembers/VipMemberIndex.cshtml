@page
@model Presentaion.Pages.VipMembers.VipMemberIndexModel

@{
    ViewData["Title"] = "VipMemberIndex";
    Layout = "_LayoutStudioPage";
}

<h2>VipMemberIndex</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VipMember[0].Customer.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VipMember[0].Customer.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VipMember[0].Customer.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VipMember[0].Studio)
            </th>
            @*<th></th>*@
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.VipMember) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Studio.Name)
            </td>
            @*<td>
               
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>*@
        </tr>
}
    </tbody>
</table>
